<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Activities">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tbar"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#FFB000"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:logo="@mipmap/ic_launcher_foreground"
        app:title="Jorell Andrei Finez"
        app:titleMargin="10dp"
        app:titleMarginStart="10dp"
        app:titleTextColor="#004225"
        android:layout_marginBottom="30dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="30dp">

        <Button
            android:id="@+id/act5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ACTIVITY 5"
            android:layout_marginBottom="16dp"/>

        <Button
            android:id="@+id/act6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ACTIVITY 6"
            android:layout_marginBottom="16dp"/>

        <Button
            android:id="@+id/act7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ACTIVITY 7"
            android:layout_marginBottom="16dp"/>

        <Button
            android:id="@+id/act8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ACTIVITY 8"
            android:layout_marginBottom="16dp"/>
    </LinearLayout>


</LinearLayout>
